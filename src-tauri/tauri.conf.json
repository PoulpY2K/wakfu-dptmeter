{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "wakfu-dptmeter",
  "version": "0.1.0",
  "identifier": "wakfu.dptmeter.anthesis",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build",
    "removeUnusedCommands": true
  },
  "app": {
    "windows": [
      {
        "decorations": true,
        "devtools": true,
        "fullscreen": false,
        "resizable": true,
        "maximized": false,
        "visible": true,
        "title": "Wakfu DPT Meter",
        "windowClassname": "Wakfu DPT Meter",
        "width": 800,
        "height": 400,
        "alwaysOnTop": true,
        "zoomHotkeysEnabled": false,
        "contentProtected": true
      }
    ],
    "security": {
      "csp": null,
      "pattern": {
        "use": "brownfield"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
